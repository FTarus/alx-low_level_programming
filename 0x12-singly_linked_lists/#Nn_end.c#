#include <string.h>
#include <stdlib.h>
#include "lists.h"

/**
 * free_list - frees a list
 * @head: head node
 * Return: void
 */

list_t *add_node_end(list_t **head, const char *str);
{
  list_t * last;
  list_t *tmp;
  
  node = malloc(sizeof(list_t));
  if (node == NULL)
     return (0);
  node->str = string;
  node->len = strlen(string);
  node->next = NULL;

  if (*head == NULL)
    *head = last;
  tmp = *head;

  while (tmp->next)
    tmp = tmp->next;
  tmp->next = last
    